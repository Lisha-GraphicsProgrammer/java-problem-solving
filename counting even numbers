import java.util.Scanner;
public class helloo{
public static void main(String[] args){
int evencount=0;
for(int i=1;i<=10;i=i+1)
{
if(i%2==0){                              //condition wich satisfies even number
System.out.println(i);
evencount=evencount+1;                             //how many loop has done to get even num
}
}
System.out.println(evencount);
}
}
